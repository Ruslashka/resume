<main>
    {{#with navigation}}
    {{#each links}}
    <a class="btn btn-link col-2" href="{{href}}">{{text}}</a>
    {{/each}}

    {{#with button}}
    <button class="btn btn-primary col-2" href="{{href}}">{{text}}</button>
    {{/with}}
    {{/with}}

    <hr>

    {{#with header}}
    <h1 class="col-3">{{title}}</h1>
    <p>{{description}}</p>
    {{#with button}}
    <a class="btn btn-primary" href="{{link}}">{{text}}</a>
    {{/with}}
    {{/with}}

    <hr>

    <section>
        <div class="row gy-3">
            {{#with city}}
            <h2>{{title}}</h2>

            {{#each cards}}
            {{#with picture}}
            <div class="card col-5">
                <img class="card-img-top" src="{{src}}" alt="{{alt}}">
                {{/with}}
                <div class="card-body">
                    <h4>{{title}}</h4>
                    <p>{{description}}</p>
                    {{#with button}}
                    <a class=" btn btn-primary btn-lg" href="{{href}}">{{text}}</a>
                    {{/with}}
                </div>
            </div>
            {{/each}}
            {{/with}}
        </div>
    </section>

    <hr>


    <section>
        {{#with winner}}
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>
                        {{this}}
                    </th>
                    {{/each}}
                </tr>
            </thead>

            <tbody>
                {{#each user}}
                <tr>
                    <th>{{place}}</th>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
            </tbody>
            {{/each}}
        </table>
        {{/with}}
        {{/with}}
    </section>

    <hr>

    <div>
        {{#with gallery}}
        <h2>{{title}}</h2>
        <div class="row">
            {{#each cards}}
            <div class="col-4 g-3">
                <div class="card">
                    <div class="card-header">
                        <div>{{name}}</div>
                        <ul class="list-group list-group-flush">
                            {{#each items}}
                            <li class="list-group-item">
                                {{this}}
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </div>

    <hr>

    <section>
        <div class="row g-3">
            {{#with relaxion}}
            <div class="display-3">{{title}}</div>
            {{#each cards}}
            <div class="card col-5 text-bg-secondary">
                <div class="card-header">
                    {{header}}
                </div>
                <div class="card-body">
                    <h4>{{title}}</h4>
                    <p>{{description}}</p>
                </div>
            </div>
            {{/each}}
            {{/with}}
        </div>
    </section>

    <hr>

    <section>
        <div class="row gy-2">
            {{#with rest}}
            {{#each cards}}
            <div class="card col-6">
                <div class="card-header text-center text-muted">
                    {{header}}
                </div>
                <div class="card-body text-center">
                    <h4>{{title}}</h4>
                    <p>{{description}}</p>
                    <button class="btn  btn-primary btn-lg">{{button}}</button>
                </div>
                <div class="card-footer text-center text-muted">
                    {{footer}}
                </div>
            </div>
            {{/each}}
            {{/with}}
        </div>
    </section>

    <hr>

    <section>
        {{#with proposition}}
        <div class="display-2">{{title}}</div>
        <br>
        <div class="row g-2">
            {{#each card}}
            <div class="card col-6 text-bg-warning">
                <div class="card-header">
                    {{header}}
                </div>
                <div class="card-body">
                    <h4>{{title}}</h4>
                    <p>{{description}}</p>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with footer}}
        <h5>{{title}}</h5>
        <div class="row">
            <div class="row col-4">
                {{#each navigation}}
                <a href=" {{href}}">{{text}}</a>
                {{/each}}
            </div>
            <br>
            <div class="row col-4">
                {{#each socialMedia}}
                <button class="btn btn-outline-dark btn-sm" href="{{link}}">{{name}}</button>
                {{/each}}
            </div>
            <br>
            <div class="row col-4">
                {{#each partners}}
                <button class="btn btn-outline-info btn-sm" href="{{link}}">{{name}}</button>
                {{/each}}
            </div>
        </div>
        {{/with}}
    </section>
</main>